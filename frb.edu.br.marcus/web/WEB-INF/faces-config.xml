<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    <navigation-rule>
        <from-view-id>/index.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>vaiParaIdiomaListagem</from-outcome>
            <to-view-id>/IdiomaListagem.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>vaiParaFilmeListagem</from-outcome>
            <to-view-id>/FilmeListagem.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/IdiomaListagem.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>voltaParaIndex</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>vaiParaIncluir</from-outcome>
            <to-view-id>/IdiomaIncluir.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>vaiParaEditar</from-outcome>
            <to-view-id>/IdiomaEditar.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>vaiParaDetalhe</from-outcome>
            <to-view-id>/IdiomaDetalhe.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>refresh</from-outcome>
            <to-view-id>/IdiomaListagem.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <managed-bean>
        <managed-bean-name>idiomaController</managed-bean-name>
        <managed-bean-class>frb.edu.br.controladores.IdiomaController</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <navigation-rule>
        <from-view-id>/IdiomaIncluir.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>voltaParaListagem</from-outcome>
            <to-view-id>/IdiomaListagem.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/IdiomaEditar.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>voltaParaListagem</from-outcome>
            <to-view-id>/IdiomaListagem.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/IdiomaDetalhe.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>voltaParaListagem</from-outcome>
            <to-view-id>/IdiomaListagem.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>vaiParaEditar</from-outcome>
            <to-view-id>/IdiomaEditar.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <managed-bean>
        <managed-bean-name>filmeController</managed-bean-name>
        <managed-bean-class>frb.edu.br.controladores.FilmeController</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <navigation-rule>
        <from-view-id>/FilmeListagem.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>voltaParaIndex</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>vaiParaIncluir</from-outcome>
            <to-view-id>/FilmeIncluir.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>vaiParaDetalhe</from-outcome>
            <to-view-id>/FilmeDetalhe.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>vaiParaEditar</from-outcome>
            <to-view-id>/FilmeEditar.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>refresh</from-outcome>
            <to-view-id>/FilmeListagem.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/FilmeIncluir.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>voltaParaListagem</from-outcome>
            <to-view-id>/FilmeListagem.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/FilmeDetalhe.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>voltaParaListagem</from-outcome>
            <to-view-id>/FilmeListagem.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>vaiParaEditar</from-outcome>
            <to-view-id>/FilmeEditar.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/FilmeEditar.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>voltaParaListagem</from-outcome>
            <to-view-id>/FilmeListagem.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
</faces-config>
